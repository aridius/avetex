<div id="cont" class="{{ class }}" itemscope itemtype="http://schema.org/Product">
<div class="row">
   <div class="col-xs-12">
      <div class="bread_scroll">
         <ul class="breadcrumb" itemscope itemtype="http://schema.org/BreadcrumbList">
            {% for i,breadcrumb in breadcrumbs %}
            {% if (i == 0) %}
            <li class="tab_mob_grandchild"><a href="{{ breadcrumb['href'] }}" class="current-link">{{ breadcrumb['text'] }}</a></li>
            {% elseif (breadcrumbs|length > i+1 ) %}
            <li class="tab_mob_grandchild" itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
               <a href="{{ breadcrumb['href'] }}" itemprop="item" itemscope itemtype="https://schema.org/Thing" itemid="{{ breadcrumb['href'] }}" title="{{ breadcrumb['text'] }}"><span itemprop="name">{{ breadcrumb['text'] }}</span></a>
               <meta itemprop="position" content="{{ i }}">
            </li>
            {% else %}
            <li class="tab_mob_grandchild" itemprop="itemListElement" itemscope itemtype="http://schema.org/ListItem">
               <span itemprop="item" itemscope itemtype="https://schema.org/Thing" itemid="{{ breadcrumb['href'] }}" title="{{ breadcrumb['text'] }}"><span itemprop="name">{{ breadcrumb['text'] }}</span></span>
               <meta itemprop="position" content="{{ i }}">
            </li>
            {% endif %}
            {% endfor %}
         </ul>
      </div>
     
      {% if (deluxe_st_cart != 1 ) %}
      <div class="st_cart">
         <div class="price_bot col-xs-3 col-sm-3 col-md-3">
            {% if (not special) %}
            <span class="price-new price">{{ price }}</span>
            {% else %}
            <span class="price-old">{{ price }}</span><span class="price-new">{{ special }}</span>
            {% endif %}
         </div>
         <div class="cart_bot col-xs-9 col-sm-9 col-md-9">
            {% if (quantity <= 0 and aridiusinstock_status ) %}
            <button type="button" class="btn-instock" onclick="instock.add('{{ product_id }}');" data-product-id="{{ product_id }}">{{ button_instock }}</button>
            {% else %}
            <button type="button" id="button-cart_st2" class="btn-cart product"><i class="fa fa-shopping-cart hidden-xs"></i> {{ button_cart }}</button>
            {% if (deluxe_forder != 1) %}
            <button type="button" class="btn-cart btn-cart__fastorder" onclick="fastOrder('{{ product_id }}');" >{{ text_fastorder }}</button>
            {% endif %}
            {% endif %}
         </div>
      </div>
      {% endif %}
   </div>
